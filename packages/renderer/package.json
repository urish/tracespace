{
  "name": "@tracespace/renderer",
  "private": true,
  "version": "4.0.2",
  "description": "Render @tracespace/plotter image trees as SVGs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracespace/tracespace.git",
    "directory": "packages/renderer"
  },
  "scripts": {
    "start": "node integration/server",
    "build": "babel src --ignore '**/__tests__/**' --out-dir lib --extensions '.ts' --root-mode upward",
    "watch": "npm run build -- --watch",
    "clean": "rm -rf lib"
  },
  "keywords": [
    "pcb",
    "circuit",
    "board",
    "hardware",
    "electronics"
  ],
  "contributors": [
    "Mike Cousins <mike@cousins.io> (https://mike.cousins.io)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tracespace/tracespace/issues"
  },
  "homepage": "https://github.com/tracespace/tracespace#readme",
  "dependencies": {
    "@tracespace/plotter": "file:../plotter",
    "@tracespace/parser": "file:../parser",
    "@tracespace/xml-id": "file:../xml-id",
    "@types/hast-format": "^2.3.0",
    "hast-util-to-html": "^6.0.2",
    "hastscript": "^5.1.0",
    "unist-util-map": "^1.0.5",
    "whats-that-gerber": "file:../whats-that-gerber"
  }
}
