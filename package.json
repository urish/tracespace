{
  "name": "tracespace",
  "private": true,
  "scripts": {
    "test": "cross-env INTEGRATION=1 nyc mocha",
    "test:watch": "mocha --watch --reporter min",
    "coverage": "nyc report --reporter=lcov",
    "start": "lerna run start --parallel",
    "prebuild": "npm run clean",
    "build": "lerna run build --parallel",
    "postbuild": "npm run types",
    "watch": "concurrently \"lerna run watch --parallel\" \"tsc --build --watch\"",
    "serve": "lerna run serve --parallel",
    "clean": "lerna run clean --parallel",
    "postclean": "npm run clean:types",
    "build:all": "npm run build && npm run example && npm run docs",
    "lint": "eslint '.*.js' '**/*.@(js|ts|tsx)'",
    "format": "prettier --ignore-path .eslintignore --write '.*.@(js|yml)' '**/*.@(js|ts|tsx|json|md|yml)'",
    "types": "tsc --build",
    "clean:types": "tsc --build --clean",
    "docs": "lerna run docs",
    "example": "lerna run example",
    "prebump": "npm run test",
    "bump": "lerna version",
    "precoverage:upload": "npm run coverage",
    "coverage:upload": "codecov",
    "publish:npm": "lerna publish from-git --yes --npm-tag $NPM_TAG",
    "publish:s3": "node ./scripts/publish-to-s3 apps/www/dist: apps/view/dist:view"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6.9.0"
  },
  "nyc": {
    "all": true,
    "include": [
      "apps/**",
      "packages/**"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/example/**",
      "**/scripts/**",
      "**/dist/**",
      "**/integration/**",
      "**/test.js",
      "**/test/**",
      "**/__tests__/**",
      "**/*.config.js",
      "config/**",
      "packages/fixtures/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".js",
      ".ts",
      ".tsx"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.2",
    "@tracespace/config": "file:config",
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/enzyme": "^3.10.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.9",
    "@types/react": "^16.9.4",
    "@types/unist": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "aws-sdk": "^2.540.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.6.1",
    "concat-stream": "^2.0.0",
    "concurrently": "^4.1.2",
    "core-js": "^3.2.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "debug": "^4.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^2.1.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "filemanager-webpack-plugin": "^2.0.5",
    "globby": "^10.0.1",
    "html-webpack-plugin": "^3.2.0",
    "lerna": "^3.14.1",
    "lodash": "^4.17.15",
    "mime": "^2.4.4",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "portfinder": "^1.0.24",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.3",
    "pump": "^3.0.0",
    "react-hot-loader": "^4.12.14",
    "run-parallel": "^1.1.9",
    "run-waterfall": "^1.1.6",
    "semver": "^6.3.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "snap-shot-it": "^7.8.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.2",
    "testdouble": "^3.12.4",
    "to-readable-stream": "^2.1.0",
    "typescript": "^3.7.0-beta",
    "unist-builder": "^1.0.4",
    "unist-util-select": "^2.0.2",
    "untildify": "^4.0.0",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@tracespace/plotter": "file:packages/plotter",
    "@tracespace/cli": "file:packages/cli",
    "@tracespace/fixtures": "file:packages/fixtures",
    "@tracespace/parser": "file:packages/parser",
    "@tracespace/renderer": "file:packages/renderer",
    "@tracespace/view": "file:apps/view",
    "@tracespace/www": "file:apps/www",
    "@tracespace/xml-id": "file:packages/xml-id",
    "gerber-parser": "file:packages/gerber-parser",
    "gerber-plotter": "file:packages/gerber-plotter",
    "gerber-to-svg": "file:packages/gerber-to-svg",
    "pcb-stackup": "file:packages/pcb-stackup",
    "pcb-stackup-core": "file:packages/pcb-stackup-core",
    "whats-that-gerber": "file:packages/whats-that-gerber"
  }
}
